{
  "name": "redux-collector",
  "version": "0.0.1",
  "description": "Easy Collection Reducers for Redux",
  "main": "lib/redux-collector.js",
  "scripts": {
    "test": "./node_modules/.bin/cross-env BABEL_ENV=commonjs ./node_modules/.bin/mocha --compilers js:babel-register --recursive",
    "compile": "./node_modules/.bin/babel --source-maps inline --optional runtime -d lib/ src/",
    "prepublish": "npm run compile",
    "ghpage": "lessc ghpage/style.less ghpage.css && pug ./ghpage/index.pug --out ./"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-1",
      "stage-2"
    ]
  },
  "nyc": {
    "exclude": [
      "*.js",
      "lib/*.js",
      "coverage/*.js",
      "**/__tests__/*.js",
      "**/__tests__/fixtures/*.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/gtg092x/redux-collector.git"
  },
  "keywords": [
    "forms",
    "react",
    "redux-collector"
  ],
  "author": "Matthew Drake <mdrake@mediadrake.com> (https://github.com/gtg092x)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gtg092x/redux-collector/issues"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "coveralls": "^2.11.8",
    "eslint": "^2.12.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-config-prometheusresearch": "^0.2.0",
    "eslint-plugin-import": "^1.8.1",
    "jsdom": "^6.3.0",
    "git-hooks": "^1.1.0",
    "jstransformer-marked": "^1.0.1",
    "mocha": "^2.3.2",
    "node-libs-browser": "^1.0.0",
    "power-assert": "^1.3.1",
    "webpack": "^1.12.14"
  },
  "dependencies": {
    "lodash": "^3.10.1",
    "redux-pipeline": "^1.0.2"
  }
}
